<?xml version="1.0" encoding="utf-8"?>
<cl.cmsg.rrhhaprobacionhrsextras.clases.CheckedLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="2dp"
    android:paddingBottom="2dp"
    android:showDividers="beginning|end"
    android:dividerPadding="1dp"
    android:divider="#ffffff"
    android:background="@drawable/listview_item_selector">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:id="@+id/layoutFondoLista">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Nombre: "
                    android:id="@+id/textView18"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:textSize="15dp"
                    android:textColor="#ffee00"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hello World!"
                    android:id="@+id/lblNombre"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:textSize="12dp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="2dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Rut:"
                            android:id="@+id/textView14"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="15dp"
                            android:textColor="#ffee00"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hello World!"
                            android:id="@+id/lblRut"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="12dp"
                            android:textColor="#ffffff"
                            android:layout_marginLeft="1dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Pacto :"
                            android:id="@+id/textView16"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="15dp"
                            android:textColor="#ffee00"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="Hello World!"
                            android:id="@+id/lblTipoPacto"
                            android:textSize="12dp"
                            android:textColor="#ffffff"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textIsSelectable="false"
                            android:shadowColor="#b6a9a9a9"
                            android:layout_marginLeft="2dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Fecha:"
                            android:id="@+id/textView15"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="15dp"
                            android:textColor="#ffee00"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hello World!"
                            android:id="@+id/lblFecha"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="12dp"
                            android:textColor="#ffffff"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="2dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Horas:"
                            android:id="@+id/textView17"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textSize="15dp"
                            android:textColor="#ffee00"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="0"
                            android:id="@+id/lblCantHoras"
                            android:textSize="12dp"
                            android:textColor="#ffffff"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:textIsSelectable="false"
                            android:shadowColor="#b6a9a9a9"
                            android:layout_marginLeft="2dp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</cl.cmsg.rrhhaprobacionhrsextras.clases.CheckedLinearLayout>
